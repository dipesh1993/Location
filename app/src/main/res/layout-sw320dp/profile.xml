<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <android.support.constraint.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#EBE4E4"
        >
        <android.support.constraint.ConstraintLayout
            android:id="@+id/relativeLayout"
            android:layout_width="match_parent"
            android:layout_height="274sp"
            android:background="@drawable/back"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <de.hdodenhof.circleimageview.CircleImageView
                android:id="@+id/imageView5"
                android:layout_width="150sp"
                android:layout_height="113sp"
                android:layout_alignParentStart="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_marginTop="35dp"
                android:src="@drawable/profile"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.498"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />



            <TextView
                android:id="@+id/textView7"
                android:layout_width="match_parent"
                android:layout_height="30sp"
                android:layout_below="@+id/imageView5"
                android:layout_alignParentStart="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentBottom="true"
                android:layout_marginStart="8sp"
                android:layout_marginLeft="8sp"
                android:layout_marginTop="8sp"
                android:layout_marginEnd="8sp"
                android:layout_marginRight="8sp"
                android:gravity="center"
                android:textColor="#010311"
                android:textSize="20sp"
                android:textStyle="bold"
                app:fontFamily="sans-serif-condensed-medium"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.507"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/imageView5" />

            <TextView
                android:id="@+id/textView8"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentBottom="true"
                android:gravity="center"
                android:textSize="16sp"
                android:textStyle="bold"
                app:fontFamily="sans-serif-black"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView7" />
        </android.support.constraint.ConstraintLayout>

        <RelativeLayout
            android:id="@+id/constraintLayout"
            android:layout_width="164sp"
            android:layout_height="170sp"
            android:layout_marginStart="10sp"
            android:layout_marginLeft="10sp"
            android:layout_marginTop="220sp"
            android:layout_marginEnd="15sp"
            android:layout_marginRight="15sp"
            android:background="@drawable/button_hover"
            android:foreground="@drawable/button"
            android:onClick="upcoming"
            app:layout_constraintEnd_toStartOf="@+id/constraintLayout2"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <ImageView
                android:id="@+id/imageView7"
                android:layout_width="97sp"
                android:layout_height="66sp"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="28sp"
                android:background="@drawable/report"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/textView9"
                android:layout_width="163sp"
                android:layout_height="36sp"
                android:layout_alignParentEnd="true"
                android:layout_alignParentBottom="true"
                android:layout_marginEnd="1sp"
                android:layout_marginBottom="5sp"
                android:background="#FFC107"
                android:gravity="center"
                android:text="Upcoming Visits"
                android:textSize="16sp"
                app:fontFamily="serif" />


        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/constraintLayout2"
            android:layout_width="164sp"
            android:layout_height="170sp"
            android:layout_marginTop="220sp"
            android:layout_marginEnd="10sp"
            android:layout_marginRight="10sp"
            android:background="@drawable/button_hover"
            android:foreground="@drawable/button"
            android:onClick="myreport"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <ImageView
                android:id="@+id/image"
                android:layout_width="97sp"
                android:layout_height="66sp"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="28sp"
                android:background="@drawable/visit"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/text"
                android:layout_width="163sp"
                android:layout_height="36sp"
                android:layout_alignParentBottom="true"
                android:layout_marginBottom="5sp"
                android:background="#FFC107"
                android:gravity="center"
                android:text="Visit Reports"
                android:textSize="16sp"
                app:fontFamily="serif" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/relativeLayout2"
            android:layout_width="164sp"
            android:layout_height="170sp"
            android:layout_marginStart="10sp"
            android:layout_marginEnd="15sp"
            android:background="@drawable/button_hover"
            android:foreground="@drawable/button"
            android:onClick="roaster"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/constraintLayout3"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/constraintLayout"
            app:layout_constraintVertical_bias="0.078">

            <ImageView
                android:id="@+id/imageView2"
                android:layout_width="97sp"
                android:layout_height="66sp"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="28sp"
                android:background="@drawable/roaster"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/textView4"
                android:layout_width="163sp"
                android:layout_height="36sp"
                android:layout_alignParentBottom="true"
                android:layout_centerHorizontal="true"
                android:layout_marginBottom="5sp"
                android:background="#FFC107"
                android:gravity="center"
                android:text="My Roaster"
                android:textSize="16sp"
                app:fontFamily="serif" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/constraintLayout3"
            android:layout_width="164sp"
            android:layout_height="170sp"
            android:layout_marginEnd="10sp"
            android:background="@drawable/button_hover"
            android:foreground="@drawable/button"
            android:onClick="mProfile"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/constraintLayout2"
            app:layout_constraintVertical_bias="0.07999998">

            <ImageView
                android:id="@+id/imageView6"
                android:layout_width="97sp"
                android:layout_height="66sp"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="28sp"
                android:background="@drawable/profile"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:layout_width="163sp"
                android:layout_height="36sp"
                android:layout_alignParentBottom="true"
                android:layout_marginBottom="5sp"

                android:background="#FFC107"
                android:gravity="center"
                android:text="Profile"
                android:textSize="16sp"
                app:fontFamily="serif" />


        </RelativeLayout>

        <ImageView
            android:id="@+id/imageView8"
            android:layout_width="match_parent"
            android:layout_height="150sp"
            android:layout_marginTop="15sp"
            android:background="@drawable/school"
            android:foreground="@drawable/school"
            android:alpha="0.8"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/relativeLayout2"

            />



    </android.support.constraint.ConstraintLayout>
</ScrollView>

